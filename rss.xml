        &lt;img class=&quot;gatsby-resp-image-image&quot; style=&quot;width: 100%; height: 100%; margin: 0; vertical-align: middle; position: absolute; top: 0; left: 0; box-shadow: inset 0px 0px 0px 400px white;&quot; src=&quot;/static/c1688298e9320cf1cbfea6900dc41d72/c856f/listen-now-full-2x.png&quot; alt=&quot;listen now with want to listen&quot; title&gt;
