        &lt;img class=&quot;gatsby-resp-image-image&quot; style=&quot;width: 100%; height: 100%; margin: 0; vertical-align: middle; position: absolute; top: 0; left: 0; box-shadow: inset 0px 0px 0px 400px white;&quot; src=&quot;/static/c331cddf389473a173382220e4f5cd1b/c856f/Want-to-Listen.png&quot; alt=&quot;want to listen carousel&quot; title&gt;
      &lt;/picture&gt;
