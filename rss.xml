    &lt;span class=&quot;gatsby-resp-image-background-image&quot; style=&quot;padding-bottom: 346.47058823529414%; position: relative; bottom: 0; left: 0; background-image: url(&amp;apos;data:image/png;base64,&amp;apos;); background-size: cover; display: block;&quot;&gt;
